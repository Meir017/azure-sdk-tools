{
  "name": "apiview",
  "version": "0.1.0",
  "private": true,
  "target": "lib",
  "publicPath": "../wwwroot/",
  "outputDir": "../wwwroot/",
  "scripts": {
    "build": "webpack --config webpack.config.js --mode=production",
    "watch": "webpack --watch",
    "tsc": "tsc --outDir ./js",
    "test": "npm run tsc && qunit test/pages/**/*.js test/shared/**/*.js"
  },
  "dependencies": {
    "@microsoft/signalr": "^7.0.5",
    "acorn": "^8.0.0",
    "core-js": "^3.3.2",
    "underscore": "^1.13.4"
  },
  "devDependencies": {
    "@types/jquery": "3.3.31",
    "@types/split.js": "^1.4.0",
    "@types/webpack-env": "^1.14.1",
    "@typescript-eslint/parser": "5.16.0",
    "bootstrap": "5.0.2",
    "css-loader": "^3.6.0",
    "eslint": "^8.11.0",
    "eslint-plugin-vue": "^8.5.0",
    "esm": "^3.2.25",
    "inspectpack": "^4.7.1",
    "jquery": "^3.6.4",
    "mini-css-extract-plugin": "^2.7.2",
    "qunit": "^2.19.4",
    "qunit-composite": "^2.0.0",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^3.3.1",
    "systemjs": "^6.14.1",
    "ts-loader": "^6.2.0",
    "typescript": "^3.6.0",
    "url": "^0.11.0",
    "webpack": "^5.82.0",
    "webpack-cli": "^5.1.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "rules": {},
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "parallel": false,
  "type": "module"
}
